{
  "build": {
    "beforeDevCommand": "make build-frontend-dev",
    "beforeBuildCommand": "elm make src/Main.elm --output=public/main.js --optimize",
    "devPath": "../public",
    "distDir": "../public",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "tauri-elm-counter",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Tauri (Vanilla) + Elm Counter",
        "width": 800,
        "height": 600
      }
    ]
  }
}
